{
  "name": "mpmgg",
  "version": "0.3.9",
  "description": "Minecraft Package Manager",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "serve": "ts-node src/index.ts",
