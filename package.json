    "@types/minimist": "^1.2.2",
    "@types/node": "^18.0.0",
    "@types/prompts": "^2.0.14",
    "ts-node": "^10.8.1",
    "tslint": "^6.1.3",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@types/cli-progress": "^3.11.0",
